    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "droidbot.start",
            "args": [
                "-a",
                "./c2d_apps/a2-todo/a22-douzifly.list_21.apk",
                "-o",
                "output2/a22b22",
                "-is_emulator",
                "-script",
                "./c2d_script/a2/b22/base/a22_drb.json",
                "-policy",
                "manual"
            ]
        }
    ]
}